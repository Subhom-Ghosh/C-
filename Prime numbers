#include <bits/stdc++.h> 
using namespace std;

int main(){
     int i,n,flag=1;
     cout<<"Enter a number ";
     cin>>n;
     for(i=2;i<=sqrt(n);i++)
     {
     	if(n%i==0){
		 
       flag=0;
       break;
   }
	 }
	 if(flag==1){
	 	cout<<n<<" is prime";
	 }
	 else{
	 	cout<<n<<" is not prime";
	 }
	 return 0;
}
